-- sound

function sfx_ui_move()
	sfx(0, 0, 0, 8)
end

function sfx_ui_act()
	sfx(1, 0, 0, 3)
end
-->8
-- util

function mod(x,m)
	return ((x+m-1) % m)+1
end

